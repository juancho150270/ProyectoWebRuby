<%= form_with(model: observer_head, local: true) do |form| %>
  <% if observer_head.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(observer_head.errors.count, "error") %> prohibited this observer_head from being saved:</h2>

      <ul>
      <% observer_head.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <div class="form-row">
      <div class="col-md-3">
        <label id="lblId">Id:</label>
        <input type="number" id="observer_head_id" name="observer_head[id]" 
        class="form-control" placeholder="Ingrese id"
        value="<%= @observer_head != nil ? @observer_head.id : '' %>">        
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-3">
        <label id="lblFecha_Creacion">Fecha_Creacion:</label>
        <input type="text" id="observer_head_fecha_creacion" name="observer_head[fecha_creacion]" 
        class="form-control" placeholder="Ingrese Fecha"
        value="<%= @observer_head != nil ? @observer_head.fecha_creacion : '' %>">        
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-3">
        <label id="lblEstudiante_id">Estudiante_id:</label>
        <input type="number" id="observer_head_estudiante_id" name="observer_head[estudiante_id]" 
        class="form-control" placeholder="Ingrese Id Estudiante"
        value="<%= @observer_head != nil ? @observer_head.estudiante_id : '' %>">        
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-3">
        <label id="lblUsuario_id">Usuario_id:</label>
        <input type="number" id="observer_head_usuario_id" name="observer_head[usuario_id]" 
        class="form-control" placeholder="Ingrese Id Usuario"
        value="<%= @observer_head != nil ? @observer_head.usuario_id : '' %>">        
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-3">
        <label id="lblTitulo">Titulo:</label>
        <input type="text" id="observer_head_titulo" name="observer_head[titulo]" 
        class="form-control" placeholder="Ingrese titulo"
        value="<%= @observer_head != nil ? @observer_head.titulo : '' %>">        
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-3">
        <label id="lblEstado">estado:</label>
        <input type="number" id="observer_head_estado" name="observer_head[estado]" 
        class="form-control" placeholder="Ingrese Estado"
        value="<%= @observer_head != nil ? @observer_head.estado : '' %>">        
      </div>
    </div>

  </div>

  <div class="form-group">
    <div class="form-row">
      <div class="col-md-12">
        
        <input type="submit" id="btnAceptar" value="Aceptar"
        class="btn btn-primary" name="btnAceptar"/>

      </div>
    </div>
  </div>  

  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>
