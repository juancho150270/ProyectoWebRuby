<p id="notice"><%= notice %></p>

<h1>Observer Details</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Fecha creacion</th>
      <th>Fecha modificacion</th>
      <th>Seguimiento academico</th>
      <th>Fecha sucesos</th>
      <th>Observacion</th>
      <th>Compromisos</th>
      <th>Obs encabezados</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @observer_details.each do |observer_detail| %>
      <tr>
        <td><%= observer_detail.id %></td>
        <td><%= observer_detail.fecha_creacion %></td>
        <td><%= observer_detail.fecha_modificacion %></td>
        <td><%= observer_detail.seguimiento_academico %></td>
        <td><%= observer_detail.fecha_sucesos %></td>
        <td><%= observer_detail.observacion %></td>
        <td><%= observer_detail.compromisos %></td>
        <td><%= observer_detail.obs_encabezados_id %></td>
        <td><%= link_to 'Show', observer_detail %></td>
        <td><%= link_to 'Edit', edit_observer_detail_path(observer_detail) %></td>
        <td><%= link_to 'Destroy', observer_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Observer Detail', new_observer_detail_path %>
