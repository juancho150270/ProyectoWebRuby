<p id="notice"><%= notice %></p>

<h1>Attendants</h1>

<table>
  <thead>
    <tr>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th>Telefono fijo</th>
      <th>Telefono celular</th>
      <th>Direccion</th>
      <th>Correo</th>
      <th>Parentesco</th>
      <th>Fecha creacion</th>
      <th>Estudiante</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendants.each do |attendant| %>
      <tr>
        <td><%= attendant.primer_nombre %></td>
        <td><%= attendant.segundo_nombre %></td>
        <td><%= attendant.primer_apellido %></td>
        <td><%= attendant.segundo_apellido %></td>
        <td><%= attendant.telefono_fijo %></td>
        <td><%= attendant.telefono_celular %></td>
        <td><%= attendant.direccion %></td>
        <td><%= attendant.correo %></td>
        <td><%= attendant.parentesco %></td>
        <td><%= attendant.fecha_creacion %></td>
        <td><%= attendant.estudiante_id %></td>
        <td><%= link_to 'Show', attendant %></td>
        <td><%= link_to 'Edit', edit_attendant_path(attendant) %></td>
        <td><%= link_to 'Destroy', attendant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendant', new_attendant_path %>
